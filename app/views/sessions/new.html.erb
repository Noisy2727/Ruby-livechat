<h1>Log in</h1>

<%= form_with url: "/login", method: :post, local: true do |form|%>
    <div>
        <%= form.label :email%>
        <p></p>
        <%= form.email_field :email %>
    </div>

    <div>
        <%= form.label :password %>
        <p></p>
        <%= form.password_field :password %>
    </div>

    <div>
        <%= form.submit "Sign in" %>
    </div>

<% end %>