<h1>Welcome <%=current_user ? current_user.username : "newcomer!"%></h1>
<% if not current_user %>
    <p>Not logged in</p>
    <%= link_to "Log in", login_path, class: "button-link"  %>
     or 
    <%= link_to "Sign up", new_user_path, class: "button-link"  %>
<% else %>
    <%= link_to "View Chatrooms", chatrooms_path, class: "button-link"  %>
<%end%>
<% if current_user%>
    <div id="bottom"> 
        <li><%= link_to "Log out", session_path(session), class: "delete-link", data: {
                turbo_method: :delete,
                turbo_confirm: "Are you sure?"
            } %></li>
    </div>
<%end%>
<%= react_component("HomePage") %>
<%= react_component("Demo") %>